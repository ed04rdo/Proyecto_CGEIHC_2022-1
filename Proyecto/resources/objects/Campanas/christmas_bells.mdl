export material christmas_bell_2_Mat(*)
  = adobe::mtl::standard(
    translucence : float(0),
    interiorColor : color(1.0, 1.0, 1.0),
    indexOfRefraction : float(1.5000),
    metallic : adobe::util::float_texture( texture_2d("christmas_bells/christmas_bell_2_metallic.png", ::tex::gamma_linear) ),
    baseColor : adobe::util::color_texture( texture_2d("christmas_bells/christmas_bell_2_baseColor.png", ::tex::gamma_srgb) ),
    roughness : adobe::util::float_texture( texture_2d("christmas_bells/christmas_bell_2_roughness.png", ::tex::gamma_linear) ),
    density : float(0),
    glow : adobe::util::float_texture( texture_2d("christmas_bells/christmas_bell_2_glow.png", ::tex::gamma_linear) ),
    opacity : adobe::util::float_texture( texture_2d("christmas_bells/christmas_bell_2_opacity.png", ::tex::gamma_linear) ),
    normal : adobe::util::normal_texture( texture_2d("christmas_bells/christmas_bell_2_normal.png", ::tex::gamma_linear) ),
    height : float(0.5),
    heightScale : float(1.0)
    );

